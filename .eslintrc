{
    "root": true,
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "prettier/react",
        "react-app"
    ],
    "plugins": [
        "@typescript-eslint/tslint"
    ],
    "settings": {
        "import/resolver": {
            "alias": [
                ["src", "./src"]
            ]
        }
    }
}
